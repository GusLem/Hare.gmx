            <string>var w = display_get_gui_width();
var h = display_get_gui_height();

if morre{
    room_speed = 10;
    image_alpha-=0.1;
    if image_alpha&lt;=0{instance_destroy();}
} else {
    if image_alpha&lt;1{image_alpha+=0.1}else if !pode{event_user(0);};
    if keyboard_check_pressed(vk_escape)&amp;&amp;pode{event_user(1);}
}

draw_set_colour(c_black);
draw_set_alpha(image_alpha);
    draw_rectangle(0,0,w,h,false);
if !morre{

draw_set_colour(c_white);
    var i = 0;
    var yy = 0.06*h;
    var altura = 0.06;
    repeat(mx){
        if (i == 4 &amp;&amp; os_is_mobile()){i++;continue;}
        var cor = c_white;
        if (alp[i] &gt; 0){
            draw_set_alpha(alp[i]);
                draw_rectangle(0,yy-h*altura,w,yy+h*altura,false);
            draw_set_alpha(1);
        }
        if (abs( (mouse_y-view_yview)-yy) &lt; h*altura &amp;&amp; !seleciona) || (sel == i &amp;&amp; seleciona){   
            mouse_mira(); 
            cor=c_ceu;
            alp[i] = clamp(alp[i]+0.03,0,1);
            pxx[i] *=0.9;
            if ((mouse_check_button_pressed(mb_left) &amp;&amp; ppode) ||
            (seleciona &amp;&amp; keyboard_check_pressed(vk_enter))) &amp;&amp;
            (!instance_exists(obj_controls_pause)){ 
                event_user(i+2);
                sfx_toca_ext(sfx_menu,1+i*0.1); 
                ini_save_escreve();
                vol_set();
            }
        }else{
            pxx[i] = min(pxx[i] + 10, w*0.2);
            alp[i] = clamp(alp[i]-0.03,0,1);
        }
        var l = "";
        switch(i){
            case 0: l = string(global.volMaster*100)+"%";break;
            case 1: l = string(global.volMusica*100)+"%";break;
            case 2: l = string(global.volSfx*100)+"%";break;
            case 3: if global.luz{l=global.pligado}else{l=global.pdesligado}break;
            
        }
        desenha_texto_ext(w*0.3 - pxx[i],yy,txt[i] + l,0,0.5,cor);
        i++;
        yy+=altura*2*h;
    }


x = mouse_x;
y = mouse_y;

if (x!=xprevious || y!=yprevious){seleciona = false;}
if keyboard_check_pressed(vk_up){
    sel--;
    if sel &lt; 0{sel = mx-1;}
    seleciona = true;
    sfx_toca_ext(sfx_menu,1+sel*0.1);
} 
if keyboard_check_pressed(vk_down){
    seleciona = true;
    sel++;
    if sel &gt;= mx{
        sel = 0;
    }
    sfx_toca_ext(sfx_menu,1+sel*0.1);
}

}
