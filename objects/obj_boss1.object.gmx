image_yscale = 1+fresp;



if hit &lt;= 0{
    draw_self();
} else {
    hit--;
    d3d_set_fog(true, c_white, 0, 0);
        draw_self();
    d3d_set_fog(false, c_black, 0, 0);
}



if (Estado == 7.1 || Estado == 10.1){
    img+=0.2;
    if (img &gt; 8) {img = 0;}
    d3d_set_fog(true, c_red, 0, 0);
        image_alpha = 0.6;
        draw_self();
        image_alpha = 1;
    d3d_set_fog(false, c_black, 0, 0);
    draw_sprite(spr_explosao1,img,
        x + sprite_width/2 + irandom_range(-10,+10),
        y + sprite_height/2 +irandom_range(-10,+10));
}





image_xscale = sign(image_xscale);
</string>
          </argument>
        </arguments>
      </action>
