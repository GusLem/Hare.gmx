    <event eventtype="3" enumb="2">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Estados
if !visible{exit;}

var w = view_wview;
var h = view_hview;

        
        
        
switch(Estado){

    
    case 0: ///inicio ESQ
        Estado++;        
    case -1:    
        image_xscale = 1;
        xx = -200;
        yy = h*0.6;
        contador++;
        if (contador&gt;120){
            Estado++;
        }
        boss1AY_movimenta();
    break;
    
    case 1: //Intro
        boss1AY_movimenta();
        xx += w*0.01;
        if (xx &gt; w*0.3){Estado++;}       
    break;
    
    case 2: //Intro 2
        boss1AY_movimenta();
        contador+=1;
        if contador &gt; 195{contador = 0; Estado++;}
    break;
    
    case 3://Comeca a ir pra la e pra ca
        boss1AY_movimenta();
        boss1AX_movimenta();
        contador+=1;
        if (contador &gt; 220){contador = 0; Estado++; vx = 0;sfx_toca_ext(sfx_ult2,1.2);}
    break;
    
    case 4://Voa para frente e transicao ataca para tras
        boss1AY_movimenta();    
        vx+=0.45;
        xx+=vx;
        contador++;
        if (contador &gt; 120){
            Estado++;    
            contador = 0;
        }
    break;
    
    
    case 5: //Posiciona DIR
        ry = choose(64,140,450);
        yy = obj_bloco_segue.y - view_yview - ry;
        xx = w+100;
        image_xscale = -1;
        Estado++;
        if ry == 140{Estado = 7;xx+=100;contador = -30; vx=-2;}
    break;
    
    case 6:// olha para o player DIR
        if (xx &gt; w*0.8){
            xx-=10;   
        } else {
            contador++;
            if (contador &gt; 10){
            /*Muda para um tipo DIR*/
                Estado = choose(11,7);
                if (ry == 450){Estado = 13;}            
                contador=0;
            }
        }
        vx = 0;
    break;
    
    case 7://rasante
        sfx_toca_ext(sfx_ult2,1.2);
        Estado=7.1;
    case 7.1:
        image_xscale = -1;
        ay*=0.8;
        vx-=0.45;
        vx = max(vx,-25);
        xx+=vx;
        contador++;
        if (contador &gt; 180){
            Estado=8;
            contador = 0;
            contador2 = 0;
        }        
    break;
    
    case 8://prepara rasante invertido
        ry = choose(64,140);
        yy = obj_bloco_segue.y - view_yview - ry;
        ay = 0;
        xx = -100;
        image_xscale = 1;
        contador = 0;
        vx = 0;
        Estado++;
        if ry == 140{Estado = 10;contador = -20;xx-=100;vx = 2;}
    break;
    
    case 9:
        if (xx &lt; w*0.03){xx+=10;} else{
            contador++;
            if (contador &gt; 30){
                /*Muda para um tipo ESQ*/
                Estado++;
                contador = 0;
                vx = 0;
            }
        
        }
    break;
    
    case 10:/*ESQ*/
        sfx_toca_ext(sfx_ult2,1.2);
        Estado=10.1;
    case 10.1:
        ay*=0.9;
        vx+=0.2;
        vx = min(vx,20);
        xx+=vx;
        contador++;
        if (contador &gt; 180){
            Estado=choose(5);
            contador = 0;
        }
    break;
    
    
    case 11: /*Atira em linha reta DIR*/
        contador2 = 0;
        contador = 0;
        dir = point_direction(x,y,obj_player.x,obj_player.y);
        Estado++;
    break;
    
    case 12:
        contador++;
        if contador &gt; 7{
            contador = 0;
            if (contador&lt;=10){
                boss2_atira(obj_b1_bala,dir,14);
            }
            contador2++;
        }
        if (contador2&gt;12){
            Estado = choose(7); vx = 0;
        }
    break;

    case 13:///Atira de cima
        dir = point_direction(x,y,obj_player.x,obj_player.y);
        Estado++;   
        contador = 0;
        contador2 = 0;
    break;
    
    case 14:
        contador++;
        if contador &gt; 7{
            contador = 0;
            if (contador&lt;=3){
                boss2_atira(obj_b1_bala,dir,14);
            }
            contador2++;
        }
        if (contador2&gt;3){
            Estado = 15; vx = 0;
        }
    break;
    
    case 15: /*Rasante de cima*/
        ty = obj_bloco_segue.y - view_yview - 64;
        contador = 0;
        contador2 = 0;
        vx = 0; 
        vy = 0;
        Estado++;
    break;
    
    case 16:
        vy+=0.45;
        if yy &lt; ty{
            yy+=vy
        } else{
            yy = ty;
            Estado = 7;
        }
    break;

}

      
if (life &gt; 0){
    x = view_xview + xx + ax;
    y = view_yview + yy + ay;
} else {//morre aqui
    if visible{
        ach_completa(0);
        //var c1 = eftx_cria_cor("I preffer Real Dragons",obj_player.id,make_colour_rgb(80,255,80),c_black);
        //eftx_comeca(c1);
        
        dorme(200);
        visible = false;
        instance_explode();
        alarm[0] = 120;    
    }
}

</string>
